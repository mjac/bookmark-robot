{
	"name": "Bookmark Title Updater",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Update bookmark titles, remove duplicates and check for broken links.",
	"options_page": "bookmarks.html",
	"permissions": [
		"http://*/",
		"https://*/",
		"bookmarks",
		"tabs"
	],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js; object-src 'self'"
}
